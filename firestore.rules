service cloud.firestore {
  match /databases/{db}/documents {
    match /quotes/{id} { 
      allow read: if true; 
      allow write: if false; 
    }
    match /meta/{doc} { 
      allow read: if true; 
      allow write: if false; 
    }
    match /users/{uid}/{any=**} { 
      allow read,write: if request.auth.uid==uid; 
    }
  }
} 